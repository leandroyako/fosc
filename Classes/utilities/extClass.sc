+ Class {
    /* --------------------------------------------------------------------------------------------------------
    • isSubclassOf

    FoscSlur.isSubclassOf(FoscSpanner);
    -------------------------------------------------------------------------------------------------------- */
    isSubclassOf { |prototype|
        var subclasses;
        subclasses = prototype.subclasses;
        if (subclasses.isNil) { ^false };
        ^(subclasses.includes(this));
    }
}
